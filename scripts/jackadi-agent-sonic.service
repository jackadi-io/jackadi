[Unit]
Description=Jackadi Agent
Documentation=https://jackadi.io/
After=network.target
Wants=network.target

[Service]
Type=simple
User=jackadi
Group=jackadi
WorkingDirectory=/var/lib/jackadi
ExecStart=/usr/bin/jackadi-agent --config /etc/sonic/jackadi/agent.yaml
Restart=on-failure
RestartSec=5
PrivateTmp=true

[Install]
WantedBy=multi-user.target
